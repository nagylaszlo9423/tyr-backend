{
  "name": "tyr-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "nest build --webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start --watch --webpack",
    "start:prod": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:services": "docker-compose -f ./config/services.docker-compose.yml up -d",
    "stop:services": "docker-compose -f ./config/services.docker-compose.yml stop",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ./test/jest-unit.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.0.13",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.13",
    "bcrypt": "^3.0.8",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "csurf": "^1.11.0",
    "file-system": "^2.2.2",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.15",
    "nest-schedule": "^0.6.4",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/testing": "^7.0.13",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.17.6",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.21",
    "@types/node": "11.13.4",
    "@types/passport": "^1.0.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/redis": "^2.8.21",
    "@types/supertest": "2.0.9",
    "builder-pattern": "^1.2.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "jest": "26.0.1",
    "mockingoose": "^2.13.2",
    "nodemon": "^2.0.4",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
